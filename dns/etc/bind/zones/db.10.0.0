;
; BIND reverse data file for local loopback interface
;
$TTL    604800
@       IN      SOA     dns.admweb.empresa.com.br. root.admweb.empresa.com.br. (
                              1         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
; name servers
        IN      NS      dns.admweb.empresa.com.br.

; PTR Records
252     IN      PTR     squid.admweb.empresa.com.br.
253     IN      PTR     dns.admweb.empresa.com.br.
4.10    IN      PTR     email.admweb.empresa.com.br.
251     IN      PTR     samba.admweb.empresa.com.br.
3.10    IN      PTR     www.admweb.empresa.com.br.
